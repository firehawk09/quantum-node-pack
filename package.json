        "publishPatch": "npm version patch && rm quantum-* && vsce package && vsce publish"
    },
    "icon": "nodejs-dark-icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/firehawk09/quantum-node-pack"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
